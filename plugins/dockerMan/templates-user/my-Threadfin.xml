<?xml version="1.0"?>
<Container version="2">
  <Name>Threadfin</Name>
  <Repository>fyb3roptik/threadfin:latest</Repository>
  <Registry>https://hub.docker.com/r/fyb3roptik/threadfin</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/133764-support-grtgbln-docker-templates</Support>
  <Project>https://github.com/Threadfin/Threadfin</Project>
  <Overview>An M3U proxy for Kernel/Plex/Jellyfin/Emby based on xTeVe</Overview>
  <Category>MediaApp:Video MediaServer:Video Other: Tools:Utilities</Category>
  <WebUI>http://[IP]:[PORT:34400]/web/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/nwithan8/unraid_templates/master/templates/threadfin.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/nwithan8/unraid_templates/master/images/threadfin-icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1721525792</DateInstalled>
  <DonateText>If you like my work, please consider donating</DonateText>
  <DonateLink>https://www.buymeacoffee.com/nwithan8</DonateLink>
  <Requires/>
  <Config Name="Web UI Port" Target="34400" Default="34400" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">34400</Config>
  <Config Name="Configuration data" Target="/home/threadfin/conf" Default="/mnt/user/appdata/threadfin/config" Mode="rw" Description="Path to configuration data" Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata/threadfin/config</Config>
  <Config Name="Temporary data" Target="/tmp/threadfin" Default="/mnt/user/appdata/threadfin/temp" Mode="rw" Description="Path to temporary data" Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata/threadfin/temp</Config>
  <Config Name="PUID" Target="PUID" Default="1001" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">1001</Config>
  <Config Name="PGID" Target="PGID" Default="1001" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">1001</Config>
</Container>
